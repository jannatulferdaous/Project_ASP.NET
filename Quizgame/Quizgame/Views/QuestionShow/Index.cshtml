@model List<QuestionShow>
@using (Html.BeginForm("Index","QuestionShow",FormMethod.Post))
{
    <div class="row">
        <h4> Play Quiz</h4>
        <hr/>
        @for (int i=0;i<Model.Count;i++)
        {
            @Html.EditorFor(model=>model[i],"QuestionShow")
            <hr/>
        }
        <input type="submit" value="Save" name="save" class="btn btn-primary" id="btnSave" />

    </div>
}


